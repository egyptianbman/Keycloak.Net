{
  "metadata": [
    {
      "src": [
        {
          "files": ["core/**.csproj", "model/**.csproj"],
          "exclude": ["**/bin/**", "**/obj/**"],
          "src": "../src",
          "properties": {
            "TargetFramework": "net6.0"
          }
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": ["api/**/*.yml", "api/**/*.md"]
      },
      {
        "files": ["articles/**/*.md", "restapi/**/*.md"]
      },
      {
        "files": ["toc.yml", "index.md"]
      }
    ],
    "resource": [
      {
        "files": ["images/**"]
      }
    ],
    "globalMetadata": {
      "_appLogoPath": "../../logo.jpg",
      "_appFaviconPath": "../../favicon.ico",
      "_enableSearch": true,
      "_disableNavbar": false,
      "_disableBreadcrumb": false,
      "_disableToc": false,
      "_disableAffix": false,
      "_disableContribution": false
    },
    "dest": "_site",
    "externalReference": [],
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": ["default"],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": true,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}
